{
  "name": "originai",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "cd packages/db && prisma generate",
    "db:push": "cd packages/db && prisma db push",
    "db:migrate": "cd packages/db && prisma migrate dev",
    "db:seed": "cd packages/db && prisma db seed",
    "db:studio": "cd packages/db && prisma studio"
  },
  "devDependencies": {
    "prettier": "^3.1.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0"
  }
}
